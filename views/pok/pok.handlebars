        function updateCell(row, index, new_value, format_uang){
            var target_td = row
                            .nodes()
                            .to$()
                            .find('td:eq('+index+')');

            tabel_pok_edit.DataTable()
                .cell(target_td)
                .data(new_value)
                .draw(false);
            if(format_uang) setAutoNumeric(target_td, new_value);
        }

