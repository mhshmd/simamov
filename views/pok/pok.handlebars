            var new_tsbi = 0;
            //init nilai realisasi bln lalu
            var new_rbl = 0;
            //init nilai sisa dana
            var sisa_dana = 0;

            //jika sum/ ingin diubah total
            if(data.sum){
                //nilai baru total sampai bulan ini
